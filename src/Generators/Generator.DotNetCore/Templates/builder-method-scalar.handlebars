        public {{ resolveBuilderName ../../ }} {{ toPascal name }}({{#with args}}{{> builder-method-args }}{{/with}})
        {
            IncludeField(
                "{{ name }}",
    {{#if args }}
                new List<Argument>
                {
        {{#each args }}
                    new Argument("{{ name }}", "{{ resolveGraphQlTypeName type }}", {{name}}),
        {{/each}}
                },
    {{else}}
                new List<Argument>(0),
    {{/if}}
                null);
            return this;
        }
